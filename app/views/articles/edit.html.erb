<h1>Editing article</h1>
<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>

<p><%= link_to 'show', article_path(@article) %> | <%= link_to 'back', articles_path %></p>
